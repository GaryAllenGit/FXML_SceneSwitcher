<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.181" xmlns:fx="http://javafx.com/fxml/1" fx:controller="complexDemo.MainSceneController">
   <children>
      <HBox alignment="CENTER" prefHeight="45.0" prefWidth="400.0">
         <children>
            <Label text="Pop Up window and Parameter Passing Demo" />
         </children>
      </HBox>
      <FlowPane alignment="CENTER" hgap="20.0">
         <children>
            <Button fx:id="performInputButton" mnemonicParsing="false" onAction="#performInputButtonClicked" text="Perform Input" />
            <Button fx:id="showOutputButton" mnemonicParsing="false" onAction="#showOutputButtonClicked" text="Show Output" />
         </children>
      </FlowPane>
   </children>
</VBox>
